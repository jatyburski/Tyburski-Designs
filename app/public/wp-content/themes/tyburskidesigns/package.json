    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "object-fit-images": "^3.2.4",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "autoprefixer-cli": "^1.0.0",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-object-fit-images": "^1.1.2"
