{
  "name": "tyburskidesigns",
  "version": "1.0.0",
  "description": "Personal portfolio website of Joseph Tyburski",
  "main": "index.js",
  "scripts": {
    "build:sass": "node-sass --output-style=expanded assets/scss/_partials.scss ../tyburskidesigns/style.css",
    "prefix": "npm run build:sass && autoprefixer-cli -o style.css style.css",
    "start": "wpackio-scripts start",
    "bootstrap": "wpackio-scripts bootstrap",
    "build": "wpackio-scripts build",
    "start-backup": "onchange \"assets/scss/**/*.scss\" -- npm run prefix",
    "archive": "wpackio-scripts pack"
  },
  "author": "Joseph Tyburski j.tyburski@yahoo.com tyburskidesigns.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/jatyburski/tyburski-designs.git"
  },
  "dependencies": {
    "@wpackio/entrypoint": "^4.4.1",
    "aos": "^2.3.4",
    "bootstrap": "^4.5.0",
    "include-media": "^1.4.9",
    "jquery": "^3.5.1",
    "less": "^3.12.2",
    "lodash": "^4.17.19",
    "object-fit-images": "^3.2.4",
    "popper.js": "^1.16.1",
    "serialize-javascript": "^4.0.0"
  },
  "devDependencies": {
    "@wpackio/scripts": "^4.4.1",
    "autoprefixer": "^9.8.0",
    "autoprefixer-cli": "^1.0.0",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-object-fit-images": "^1.1.2"
  }
}
